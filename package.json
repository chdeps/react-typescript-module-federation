{
  "name": "container",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "ts-loader": "^9.1.2",
    "html-webpack-plugin": "5.3.2",
    "serve": "12.0.1",
    "typescript": "4.3.5",
    "webpack": "5.61.0",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "^4.4.0"
  },
  "scripts": {
    "start:container": "webpack-cli serve",
    "start:page1": "cd ./src/pages/page1 && webpack-cli serve",
